{
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "files": [
		
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests",".git"],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "bin/artist",
    "output": "artist.phar",
    "stub": true,
	
	"algorithm": "OPENSSL",
	"compression": "GZ",
	"intercept": true,
	"key": ".travis/phar-private.pem"
}